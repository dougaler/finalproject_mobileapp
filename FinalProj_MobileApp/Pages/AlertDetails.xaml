<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FinalProj_MobileApp.Pages.AlertDetails"
             xmlns:vm="clr-namespace:FinalProj_MobileApp.Models.ViewModels"
             Title="AlertDetails">
    <TableView Intent="Data">
        <TableRoot>
            <TableSection x:Name="LblTitle">
                <TextCell x:Name="LblDate" Text="Date: "/>
                <TextCell x:Name="LblSeverity" Text="Severity: "/>
                <TextCell x:Name="LblStatus" Text="Status: "/>
                <TextCell x:Name="LblDescription" Text="Description: "/>
            </TableSection>
        </TableRoot>
    </TableView>
</ContentPage>