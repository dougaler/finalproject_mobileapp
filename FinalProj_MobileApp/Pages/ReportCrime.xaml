<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FinalProj_MobileApp.Pages.ReportCrime"
             Title="ReportCrime">
    <VerticalStackLayout Padding="10" Spacing="10">
        <Entry x:Name="name" Placeholder="Name" Text="Binding Stuff Place Holder"/>
        <Entry x:Name="email" Placeholder="Email" Text="Binding Stuff Place Holder"/>
        <Entry x:Name="phoneNumber" Placeholder="Phone Number" Text="Binding Stuff Place Holder"/>
        <Entry x:Name="description" Placeholder="Description" Text="Binding Stuff Place Holder"/>
        <!-- <Button x:Name="addButton" Text="Add Contact" Command="{Binding AddCommand}"/> 
        <Button Text="View List" Clicked="Button_Clicked"/> -->
    </VerticalStackLayout>
</ContentPage>